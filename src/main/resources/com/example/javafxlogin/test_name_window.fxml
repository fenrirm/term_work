<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="210.0" prefWidth="280.0" style="-fx-background-color: #b7f9ff;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafxlogin.TestNameWindowController">
   <children>
      <Label layoutX="9.0" layoutY="24.0" prefHeight="25.0" prefWidth="77.0" text="Name:">
         <font>
            <Font name="Consolas Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="9.0" layoutY="72.0" prefHeight="25.0" prefWidth="77.0" text="Deadline:">
         <font>
            <Font name="Consolas Bold" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="tfTestName" layoutX="105.0" layoutY="24.0" prefHeight="25.0" prefWidth="167.0" promptText="Enter test name">
         <font>
            <Font name="Consolas Bold" size="12.0" />
         </font>
      </TextField>
      <TextField fx:id="tfTestDeadline" layoutX="105.0" layoutY="72.0" prefHeight="25.0" prefWidth="167.0" promptText="Enter test deadline">
         <font>
            <Font name="Consolas Bold" size="12.0" />
         </font>
      </TextField>
      <Button fx:id="saveButton" layoutX="101.0" layoutY="174.0" mnemonicParsing="false" prefHeight="22.0" prefWidth="77.0" style="-fx-background-color: #000000;" text="Save" textFill="WHITE">
         <font>
            <Font name="Consolas Bold" size="12.0" />
         </font>
      </Button>
      <Label layoutX="9.0" layoutY="120.0" prefHeight="25.0" prefWidth="90.0" text="Phone number:">
         <font>
            <Font name="Consolas Bold" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="tfAuthorNumber" layoutX="105.0" layoutY="120.0" prefHeight="25.0" prefWidth="167.0" promptText="Enter your phone number">
         <font>
            <Font name="Consolas Bold" size="12.0" />
         </font>
      </TextField>
   </children>
</AnchorPane>
